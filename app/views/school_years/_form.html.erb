<%= form_with model: @school_year, local: true do |form| %>
    <div>
      <%= form.label :name %>
      <%= form.text_field :name %>
    </div>
  
    <div>
      <%= form.label :start_date %>
      <%= form.date_field :start_date %>
    </div>
  
    <div>
      <%= form.label :end_date %>
      <%= form.date_field :end_date %>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
  <% end %>
  